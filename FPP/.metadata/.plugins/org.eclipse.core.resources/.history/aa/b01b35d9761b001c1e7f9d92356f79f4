import java.util.Arrays;
import java.util.HashSet;
import java.util.List;



public class SetPractice {
	
	public static void main(String[] args) {
		List<String> list1=Arrays.asList("Milon","Kiron", "Sazed");
		List<String> list2=Arrays.asList("Mahir","Nayem");
		//list1.addAll(list2);
		
		HashSet<String> setObj=new HashSet<>();
		setObj.add(null);
		setObj.add("Abdul");
		setObj.add("Sarif");
		setObj.add("Rimon");
		setObj.add("Babul");
		setObj.add("Horon");
		setObj.add("himal");
		
		System.out.println(setObj.contains("Sarif"));
		System.out.println(setObj.isEmpty());
		System.out.println(setObj.size());
		System.out.println(setObj.remove("himal"));
		System.out.println(setObj.size());
		setObj.addAll(list1);
		
	}
	
}
