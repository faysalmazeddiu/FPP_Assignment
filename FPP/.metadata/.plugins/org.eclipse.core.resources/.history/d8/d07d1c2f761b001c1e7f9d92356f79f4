import java.util.Arrays;
import java.util.HashSet;
import java.util.List;



public class SetPractice {
	
	public static void main(String[] args) {
//		List<String> list1=Arrays.asList(21,3,4,5,61,78);
//		List<String> list2=Arrays.asList(21,3,1,5,3,4);
		
		HashSet<String> setObj=new HashSet<>();
		setObj.add(null);
		setObj.add("Abdul");
		setObj.add("Sarif");
		setObj.add("Rimon");
		setObj.add("Babul");
		setObj.add("Horon");
		setObj.add("himal");
		
		System.out.println(setObj.contains("Sarif"));
	}
	
}
